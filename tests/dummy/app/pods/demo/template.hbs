{{#frost-list
  class='frost-flex-1'
  on-scroll-y-end=(action 'yEndReached')
  on-select=(action 'selected')
  records=model
  selections=selectedItems
  as |record|
}}
  {{!#if (eq record.record-type 'custom')}}
    {{component componentPath model=record detailLevel=activeDetailLevel}}
  {{!/if}}
{{/frost-list}}
