{{frost-list
  class='demo'
  hook='demo'
  itemTypes=(hash
    a=(hash
      item='listItemTypeA'
      itemExpansion='listItemTypeAExpansion'
    )
    b=(hash
      item='listItemTypeB'
    )
  )
  itemTypeKey='itemType'
  itemDefinitions=(hash
    listItemTypeA=(component 'list-item-type-a')
    listItemTypeB=(component 'list-item-type-b')
  )
  itemExpansionDefinitions=(hash
    listItemTypeAExpansion=(component 'list-item-type-a-expansion')
  )
  item=(component 'list-item-typed')
  itemExpansion=(component 'list-item-typed-expansion')
  items=items
  expandedItems=expandedItems
  selectedItems=selectedItems
  onExpansionChange=(action 'onExpansionChange')
  onSelectionChange=(action 'onSelectionChange')
  sorting=(component 'frost-sort'
    sortOrder=sortOrder
    sortingProperties=sortingProperties
    onChange=(action 'onSortingChange')
  )
}}
